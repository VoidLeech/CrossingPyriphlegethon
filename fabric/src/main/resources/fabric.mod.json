{
    "schemaVersion": 1,
    "id": "crossing_pyriphlegethon",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "${homepage}",
        "issues": "${homepage}/issues"
    },
    "license": "${license}",
    "icon": "icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.github.voidleech.crossing_pyriphlegethon.CrossingPyriphlegethonFabric"
        ],
        "client": [
            "com.github.voidleech.crossing_pyriphlegethon.CrossingPyriphlegethonFabricClient"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14",
        "fabric": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=17",
        "oblivion_api": ">=${oblivion_version}"
    },
    "suggests": {
        "another-mod": "*"
    },
    "custom": {
        "modmenu": {
            "badges": [ ]
        }
    }
}
  